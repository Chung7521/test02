import pandas as pd

se = pd.Series((4.1, 3.4, 3.8, 3.3, 3.5))
print(se)
print("len(se) :", len(se), se[1:4])
print(se[1], " - ", se.values, " - ", se.values[2:], " - ", se.index)
print("se.size :", se.size, " - ", se.ndim, " - ", se.shape, " - ", se.dtype)
