import pandas as pd
import numpy as np

x = [1, 2, 3]
y = [4, 5, 6]
z = [7, 8, 9]

arr = np.concatenate([x, y, z])
print(arr)
print()

x = [[1, 2], [3, 4]]
arr2 = np.concatenate([x, x], axis=0)
print(arr2)
print()
arr3 = np.concatenate([x, x], axis=1)
print(arr3)
print()

ser1 = pd.Series(["a", "b", "c"], index=[1, 2, 3])
ser2 = pd.Series(["d", "e"], index=[4, 5])
ser3 = pd.concat([ser1, ser2])
print(ser3)
