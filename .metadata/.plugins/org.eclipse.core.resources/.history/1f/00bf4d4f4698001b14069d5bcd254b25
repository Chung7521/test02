import pandas as pd
import numpy as np

df = pd.DataFrame({"a":np.random.rand(5),
                            "b":np.random.rand(5)})
print(df)
print()

df1 = pd.DataFrame({"key": ["a", "b", "c", "a", "b", "c"],
                    "data": range(6)}, columns=["key", "data"])
print(df1)
