import pandas as pd
import numpy as np

np_df = pd.DataFrame(np.random.randint(10, 20, size=(3, 3)),
                     columns=["A", "B", "C"],
                     index=[0, 1, 2])
print(np_df)
print()

np_df["D"] = [100, 200, 300]
print("np_df : \n{}\n".format(np_df))

print(np_df.values)
print(np_df.values[0])
print()

print(np_df.T)
print()
print(np_df)





