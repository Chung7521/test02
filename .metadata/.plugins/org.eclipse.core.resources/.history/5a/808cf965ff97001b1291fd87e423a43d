import pandas as pd

pop = pd.Series({"강서구": 445200, "구로구": 418700, "양천구": 466900, "관악구": 492600})
area = pd.Series({"강서구": 3830, "구로구": 3370, "양천구": 3520, "동작구": 3280})
states = pd.DataFrame({"population":pop, "area":area})
print(states)
print()

states.columns = ["pop", "area"]
print(states)
print()
print(states["pop"])
print()
print(states["pop"].values)