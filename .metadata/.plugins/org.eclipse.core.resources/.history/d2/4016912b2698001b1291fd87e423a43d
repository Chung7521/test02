import re

p = re.compile("(ABC)+")
s = "ABCDEABDABCFABC"
print(p.findall(s))
print()

p = re.compile("(ABC)+")
s = "ABCDEABDABCFABCAACABCABCCCABC"
print(p.findall(s))
print()

p1 = re.compile(r"\w+\s+\d+[-]\d[-]\d+")
p2 = re.compile("\w+\s+\d+[-]\d[-]\d+")
print(p1.search("홍길동 010-8971-5623").group())
print(p2.search("홍길동 010-8971-5623").group())