import pandas as pd
import numpy as np

df = pd.DataFrame({"a":np.random.rand(5),
                            "b":np.random.rand(5)})
print(df)
print()

print(df.sum())
print(df.mean())
print(df.median())
print(df.std())
print(df.var())
print(df.min())
print(df.max())
print(df.count())
print()

print(df["a"].sum())
print()

print(df.describe())