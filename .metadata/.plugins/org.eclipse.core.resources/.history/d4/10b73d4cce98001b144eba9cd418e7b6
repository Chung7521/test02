import matplotlib.pyplot as plt
import numpy as np

def chartLineStyle():
    
    x = np.linspace(0, 10, 1000)
    
    plt.plot(x, np.sin(x - 0), color="blue", label="solid", linestyle="solid", linewidth=3)
    plt.plot(x, np.sin(x - 1), color="g", label="dashed", linestyle="dashed", lw=3)
    
    plt.show()

