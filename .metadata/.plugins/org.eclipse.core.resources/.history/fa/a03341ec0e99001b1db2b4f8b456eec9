import csv
import matplotlib.pyplot as plt
import numpy as np

with open("temperature.csv", "r") as a:    
    tem = csv.reader(a)
    next(tem)
    
    birth = []
    max = []
    min = []
    
    for row in tem:
        #print(row[0])
        
        date = row[0].split("-")
        #print(date)
        
        if int(date[0]) >= 1998:
            
            if date[1] == "02" and date[2] == "24":
                
                birth.append(row[0])
                #print(birth)
                max.append(row[5]))